<tr class='admin_row <%= cycle("even", "odd") %>'>
  <td>
    <%= superinquiry.title.humanize %>
  </td>
  <td>
    <%= link_to refinery_icon_tag("application_go.png"), refinery.superinquiries_superinquiry_path(superinquiry),
        :title => t('.view_live_html'),
        :target => "_blank" %>
  </td>
  <td>
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_superinquiries_admin_superinquiry_path(superinquiry),
         :title => t('.edit') %>
  </td>
  <td>
    <%= link_to refinery_icon_tag("delete.png"), refinery.superinquiries_admin_superinquiry_path(superinquiry),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => superinquiry.title),
        :method => :delete %>
  </td>
</tr>